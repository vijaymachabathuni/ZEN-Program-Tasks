3. Name, Capital and Flag of countries
   Code : var xhr = new XMLHttpRequest();
          xhr.open("GET", "https://restcountries.eu/rest/v2/all");
          xhr.onload = function () {
            var data = JSON.parse(this.response);
            data.forEach( x => {
              console.log(data.indexOf(x), x.name, x.capital, x.flag)      
            });
          };
          xhr.send();

