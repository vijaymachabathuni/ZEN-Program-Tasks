4. Total Population of countries
   Code : var xhr = new XMLHttpRequest();
          xhr.open("GET", "https://restcountries.eu/rest/v2/all");
          xhr.onload = function () {
            var data = JSON.parse(this.response);
            console.log( 'Total Population', '=', data.map( x => parseInt(x.population)).
              reduce((total, current) => total + current));
          };
          xhr.send();

   Output : Total Population = 7349137231
   
   
5. Countries with currency US dollars
   Code : var xhr = new XMLHttpRequest();
          xhr.open("GET", "https://restcountries.eu/rest/v2/all");
          xhr.onload = function () {
            var data = JSON.parse(this.response);
            console.log(data.filter((x) => x.currencies[0].code === "USD"));
          };
          xhr.send();

   Output : 0: {name: "American Samoa", topLevelDomain: Array(1), alpha2Code: "AS", alpha3Code: "ASM", callingCodes: Array(1), …}
