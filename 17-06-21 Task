2. var xhr = new XMLHttpRequest();
       xhr.open("GET", "https://restcountries.eu/rest/v2/all");
       xhr.onload = function () {
           var data = JSON.parse(this.response);
           for( var i in data){
           console.log(i + " : " + data[i].flag)   
       }
   };
   xhr.send();
   
