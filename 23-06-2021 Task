1. aa = (f, s, t) => {
       if (f > s && f > t) {
           console.log(f)
       }
       else if (s > f && s > t) {
           console.log(s)
       }
       else {
           console.log(t)
       }
   }
   aa(1, 2, 3);
   
2. let n = 123;
   function add(n) {
       let sum = 0;
       while(n>0){
           sum += n%10
           n = parseInt(n/10)
       }
       return sum;
   }
   console.log(add(n));   
   
